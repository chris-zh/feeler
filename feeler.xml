  <!-- Modules -->
  
  <import file="${basedir}/module_feeler.xml"/>
  
  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.feeler, clean.artifact.feeler:war_exploded" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.feeler" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.feeler:war" value="${basedir}/target"/>
    <property name="artifact.output.feeler:war_exploded" value="${basedir}/target/feeler"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.feeler.war" value="${artifacts.temp.dir}/feeler.war"/>
  </target>
  
  <target name="clean.artifact.feeler:war_exploded" description="clean feeler:war exploded artifact output">
    <delete dir="${artifact.output.feeler:war_exploded}"/>
  </target>
  
  <target name="artifact.feeler:war" depends="init.artifacts, artifact.feeler:war_exploded" description="Build &#39;feeler:war&#39; artifact">
    <property name="artifact.temp.output.feeler:war" value="${artifacts.temp.dir}/feeler_war"/>
    <mkdir dir="${artifact.temp.output.feeler:war}"/>
    <zip destfile="${temp.jar.path.feeler.war}">
      <zipfileset dir="${artifact.output.feeler:war_exploded}"/>
    </zip>
    <copy file="${temp.jar.path.feeler.war}" tofile="${artifact.temp.output.feeler:war}/feeler.war"/>
  </target>
  
  <target name="artifact.feeler:war_exploded" depends="init.artifacts, compile.module.feeler" description="Build &#39;feeler:war exploded&#39; artifact">
    <mkdir dir="${artifact.output.feeler:war_exploded}"/>
    <mkdir dir="${artifact.output.feeler:war_exploded}/WEB-INF"/>
    <mkdir dir="${artifact.output.feeler:war_exploded}/WEB-INF/classes"/>
    <copy todir="${artifact.output.feeler:war_exploded}/WEB-INF/classes">
      <fileset dir="${feeler.output.dir}"/>
    </copy>
    <mkdir dir="${artifact.output.feeler:war_exploded}/WEB-INF/lib"/>
    <copy file="${path.variable.maven_repository}/junit/junit/4.8.1/junit-4.8.1.jar" tofile="${artifact.output.feeler:war_exploded}/WEB-INF/lib/junit-4.8.1.jar"/>
    <copy file="${path.variable.maven_repository}/org/apache/maven/plugins/maven-resources-plugin/2.4.3/maven-resources-plugin-2.4.3.jar" tofile="${artifact.output.feeler:war_exploded}/WEB-INF/lib/maven-resources-plugin-2.4.3.jar"/>
    <copy file="${path.variable.maven_repository}/org/apache/maven/maven-plugin-api/2.0.6/maven-plugin-api-2.0.6.jar" tofile="${artifact.output.feeler:war_exploded}/WEB-INF/lib/maven-plugin-api-2.0.6.jar"/>
    <copy file="${path.variable.maven_repository}/org/apache/maven/maven-project/2.0.6/maven-project-2.0.6.jar" tofile="${artifact.output.feeler:war_exploded}/WEB-INF/lib/maven-project-2.0.6.jar"/>
    <copy file="${path.variable.maven_repository}/org/apache/maven/maven-settings/2.0.6/maven-settings-2.0.6.jar" tofile="${artifact.output.feeler:war_exploded}/WEB-INF/lib/maven-settings-2.0.6.jar"/>
    <copy file="${path.variable.maven_repository}/org/apache/maven/maven-profile/2.0.6/maven-profile-2.0.6.jar" tofile="${artifact.output.feeler:war_exploded}/WEB-INF/lib/maven-profile-2.0.6.jar"/>
